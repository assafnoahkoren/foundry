{
  "name": "server",
  "version": "0.0.1",
  "description": "Fastify + tRPC server",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/worker.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:safe": "node test-with-db.js",
    "test:safe:ui": "node test-with-db.js --ui",
    "test:db:setup": "tsx src/test/setup-test-db-prisma.ts --setup",
    "test:db:teardown": "tsx src/test/setup-test-db-prisma.ts --teardown",
    "email:preview": "node src/services/mail/watch-email-templates.js"
  },
  "dependencies": {
    "@bull-board/api": "^6.12.0",
    "@bull-board/fastify": "^6.12.0",
    "@elevenlabs/elevenlabs-js": "^2.13.0",
    "@fastify/cors": "^11.0.1",
    "@prisma/client": "^6.12.0",
    "@react-email/components": "^0.3.2",
    "@react-email/render": "^1.1.3",
    "@trpc/server": "^10.45.2",
    "@types/glob": "^8.1.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.56.5",
    "dotenv": "^17.2.0",
    "fastify": "^5.4.0",
    "glob": "^11.0.3",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.8",
    "openai": "^5.10.2",
    "prisma": "^6.12.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@react-email/preview-server": "^4.2.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv-cli": "^8.0.0",
    "globals": "^16.3.0",
    "pg": "^8.16.3",
    "react-email": "^4.2.3",
    "supertest": "^7.1.3",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.2.4"
  }
}
