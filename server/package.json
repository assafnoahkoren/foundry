{
  "name": "server",
  "version": "0.0.1",
  "description": "Fastify + tRPC server",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "prisma:generate": "dotenv -e ../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../.env -- prisma migrate dev",
    "prisma:studio": "dotenv -e ../.env -- prisma studio",
    "prisma:push": "dotenv -e ../.env -- prisma db push"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@prisma/client": "^6.12.0",
    "@trpc/server": "^10.45.2",
    "@types/pg": "^8.15.4",
    "bcryptjs": "^3.0.2",
    "fastify": "^4.26.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prisma": "^6.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "dotenv-cli": "^8.0.0",
    "globals": "^16.3.0",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.37.0"
  }
}
